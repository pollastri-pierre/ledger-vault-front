{
  "name": "ledger-vault-front",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production webpack --progress --hide-modules --config webpack/build.babel.js",
    "coverage": "npm run test -- --coverage",
    "flow": "flow",
    "cypress:open": "cypress open",
    "cypress:run:onboarding_fast": "cypress run --spec 'cypress/integration/Onboarding_Fast/*' --browser chrome",
    "cypress:run:onboarding": "cypress run --spec 'cypress/integration/Onboarding/*' --browser chrome",
    "cypress:run:onboarding_wipe": "cypress run --spec 'cypress/integration/Onboarding_Wipe/*' --browser chrome",
    "cypress:run:account": "cypress run --spec 'cypress/integration/Account/*' --browser chrome",
    "cypress:run:operation": "cypress run --spec 'cypress/integration/Operation/*' --browser chrome",
    "lint": "eslint src test webpack",
    "flow-typed": "bash scripts/install-flow-typed.sh",
    "prettier": "prettier --write \"{src,webpack,test,cypress,.storybook}/**/*.{js,json}\"",
    "start": "NODE_ENV=development webpack-dev-server --https --config webpack/dev.babel.js",
    "starte2e": "NODE_ENV=e2e webpack-dev-server --https --config webpack/dev.babel.js",
    "test": "jest --env=jsdom --maxWorkers 2",
    "device": "node scripts/device.js",
    "testDev": "jest --env=jsdom --watch",
    "storybook": "start-storybook -p 6006 --https --ssl-cert localhost.crt --ssl-key localhost.key",
    "build-storybook": "build-storybook",
    "publish-storybook": "bash scripts/publish-storybook.sh"
  },
