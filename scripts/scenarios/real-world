# ethereum account
python test_account_creation.py \
  --name "Blockchain ETH" \
  --currency ethereum \
  --approve \
  --multi-sets-json "[
    {
        \"name\": \"Rule 1\",
        \"rules\": [
            {
                \"type\": \"WHITELIST\",
                \"data\": [1]
            },
            {
                \"type\": \"THRESHOLD\",
                \"data\": [
                    {
                        \"currency_type\": \"CRYPTO\",
                        \"min\": \"0\",
                        \"max\": \"1000\"
                    }
                ]
            },
            {
                \"type\": \"MULTI_AUTHORIZATIONS\",
                \"data\": [
                    {\"quorum\": 1, \"group_id\": 3}
                ]
            }
        ]
    },
    {
        \"name\": \"Rule 2\",
        \"rules\": [
            {
                \"type\": \"MULTI_AUTHORIZATIONS\",
                \"data\": [
                    {\"quorum\": 2, \"group_id\": 3},
                    {\"quorum\": 2, \"users\": [13, 14]}
                ]
            },
            {
                \"type\": \"THRESHOLD\",
                \"data\": [
                    {
                        \"currency_type\": \"CRYPTO\",
                        \"min\": \"0\",
                        \"max\": \"1000\"
                    }
                ]
            }
        ]
    },
    {
        \"name\": \"Rule 3\",
        \"rules\": [
            {
                \"type\": \"MULTI_AUTHORIZATIONS\",
                \"data\": [
                    {\"quorum\": 2, \"group_id\": 3},
                    {\"quorum\": 1, \"users\": [14]}
                ]
            },
            {
                \"type\": \"THRESHOLD\",
                \"data\": [
                    {
                        \"currency_type\": \"CRYPTO\",
                        \"min\": \"2000\",
                        \"max\": \"4000\"
                    }
                ]
            }
        ]
    }
]"
