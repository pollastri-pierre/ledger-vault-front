  --name 'America Ops' \
  --approve \
  --users 13 \
  --users 14 \
  --users 16

# bitcoin account
python test_account_creation.py \
  --name BlockchainPlanet \
  --currency bitcoin \
  --approve \
  --multi-sets-json "[
    { \"name\": \"Rule 1\",
      \"rules\": [
          { \"type\": \"MULTI_AUTHORIZATIONS\",
            \"data\": [
                { \"quorum\": 2, \"users\": [13, 14]},
                { \"quorum\": 2, \"group_id\": 3}
            ]
          }
      ]
    }
  ]"

# ethereum account
